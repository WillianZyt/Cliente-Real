<form method="post" action="{{ path('app_admin_banner_delete', {'id': banner.id}) }}" onsubmit="return confirm('Quer mesmo excluir este item??');">
	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ banner.id) }}">
	<button class="btn btn-danger ms-3 mt-3 float-start text-white">Excluir</button>
</form>
